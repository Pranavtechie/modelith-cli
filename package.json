		"@types/bun": "^1.2.9",
		"@types/d3": "^7.4.3",
		"@types/jsdom": "^21.1.7",
		"@types/react": "^19.1.1",
		"@types/react-dom": "^19.1.2",
		"@vitejs/plugin-react": "^4.3.4",
		"jsdom": "^26.1.0",
		"typescript": "^5.8.3",
		"vite": "^6.2.6",
		"vitest": "^3.1.1",
		"web-vitals": "^4.2.4"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@radix-ui/react-slot": "^1.2.0",
		"@tailwindcss/vite": "^4.1.3",
		"@tanstack/react-router": "^1.116.0",
