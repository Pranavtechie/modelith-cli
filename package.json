		"@types/inquirer": "^9.0.7",
		"chalk": "^5.4.1",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"commander": "^13.1.0",
		"csv-parse": "^5.6.0",
		"d3": "^7.9.0",
		"drizzle-kit": "^0.30.6",
		"drizzle-orm": "^0.41.0",
		"env-paths": "^3.0.0",
		"exiftool-vendored": "^29.3.0",
		"inquirer": "^12.5.2",
		"lucide-react": "^0.476.0",
		"ora": "^8.2.0",
