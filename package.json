{
  "name": "modelith",
  "module": "src/index.ts",
  "type": "module",
  "version": "0.2.4",
  "license": "MIT",
  "bin": "./cli-dist/index.js",
  "scripts": {
    "start": "bun run --bun vite --port 3000",
    "build:frontend": "bun x --bun vite build",
    "build:cli": "bun build src/index.ts --outdir cli-dist --target bun --sourcemap --external playwright --external playwright-core --external tree-sitter --external tree-sitter-python --external exiftool-vendored --external jsdom --external svg2img",
    "build:backend": "bun build src/backend/src/index.ts --outdir backend-dist --target bun --sourcemap",
    "dev": "bun run src/index.ts",
    "start:backend": "bun run src/backend/src/index.ts",
    "start:frontend": "bun run --bun vite --port 3000",
    "start:dev": "concurrently \"bun run start:backend\" \"bun run start:frontend\"",
    "preview": "bun x --bun vite preview",
    "generate": "bun x --bun drizzle-kit generate --config drizzle.config.ts",
    "build": "bun run build:cli && bun run build:frontend && bun run build:backend"
  },
  "packageManager": "bun@1.2.10",
  "files": [
    "drizzle/**/*",
    "cli-dist",
    "backend-dist",
    "dist",
    "bun.lock",
    "package.json",
    "components.json",
    "drizzle.config.ts",
    "LICENSE",
    "tsconfig.json",
    "vite.config.ts"
  ],
  "devDependencies": {
    "@libsql/client": "^0.15.3",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "^1.2.9",
    "@types/d3": "^7.4.3",
    "@types/inquirer": "^9.0.7",
    "@types/jsdom": "^21.1.7",
    "@types/react": "^19.1.1",
    "@types/react-dom": "^19.1.2",
    "concurrently": "^9.1.2",
    "serve": "^14.2.4",
    "typescript": "^5.8.3",
    "vitest": "^3.1.1",
    "web-vitals": "^4.2.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.2.0",
    "@floating-ui/react": "^0.27.8",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.4",
    "@shadcn/ui": "^0.0.4",
    "@tailwindcss/vite": "^4.1.3",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/react-router": "^1.116.0",
    "@tanstack/react-router-devtools": "^1.116.0",
    "@tanstack/router-plugin": "^1.116.1",
    "@trpc/client": "^11.1.1",
    "@trpc/react-query": "^11.1.1",
    "@trpc/server": "^11.1.1",
    "@uiw/react-markdown-preview": "^5.1.3",
    "@vitejs/plugin-react": "^4.3.4",
    "chalk": "^5.4.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "commander": "^13.1.0",
    "csv-parse": "^5.6.0",
    "d3": "^7.9.0",
    "drizzle-kit": "^0.30.6",
    "drizzle-orm": "^0.41.0",
    "elysia": "latest",
    "env-paths": "^3.0.0",
    "inquirer": "^12.5.2",
    "jsdom": "^26.1.0",
    "ora": "^8.2.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-syntax-highlighter": "^15.6.1",
    "sharp": "^0.34.1",
    "svg2img": "^1.0.0-beta.2",
    "tailwind-merge": "^3.2.0",
    "tailwindcss": "^4.1.3",
    "tailwindcss-animate": "^1.0.7",
    "tree-sitter": "^0.22.4",
    "tree-sitter-python": "^0.23.6",
    "vite": "^6.2.6",
    "zod": "^3.24.3",
    "zod-to-ts": "^1.2.0"
  },
  "trustedDependencies": [
    "@fortawesome/fontawesome-free",
    "esbuild",
    "sharp",
    "tree-sitter",
    "tree-sitter-python"
  ],
  "optionalDependencies": {
    "exiftool-vendored": "^29.3.0",
    "playwright": "^1.51.1"
  }
}
